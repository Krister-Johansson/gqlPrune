{
  "name": "eslint-plugin-unused-graphql-operations",
  "version": "1.0.1",
  "description": "This script helps identify unused GraphQL operations (queries, mutations, subscriptions) in your project. It scans .gql files for operations and checks if they are being used in your TypeScript/JavaScript files.",
  "bin": {
    "gqlPrune": "./dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "start": "node ./dist/cli.js",
    "dev": "npm run build && npm run start",
    "test": "jest",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,ts,tsx}'"
  },
  "keywords": [],
  "author": "Krister Johansson",
  "license": "MIT",
  "dependencies": {
    "graphql": "^16.8.1",
    "inquirer": "^9.2.11",
    "js-yaml": "^4.1.0",
    "kleur": "^4.1.5"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/inquirer": "^9.0.4",
    "@types/jest": "^29.5.5",
    "@types/js-yaml": "^4.0.7",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
